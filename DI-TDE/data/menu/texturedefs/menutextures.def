		Rect("frame_a_top_left"    , 0, 0, 86, 86) // 86x86
		Rect("frame_a_top_right"   , 170, 0, 256, 86) // 86x86
			
		CornerTopLeft("frame_a_top_left",			100,0)
		CornerTopRight("frame_a_top_right",			100,0)
		CornerBottomLeft("frame_a_bottom_left",		100,0)
		CornerBottomRight("frame_a_bottom_right",	100,0)
			
		Top("frame_a_top",		100,0)
		Bottom("frame_a_bottom",100,0)
